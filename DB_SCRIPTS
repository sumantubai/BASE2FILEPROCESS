create or replace PROCEDURE          sp_ins_mdureco (errmsg OUT VARCHAR2)
AS
   v_temp1             VARCHAR2 (3)                               := 'P1';
   v_lastmdufiledate   VARCHAR2 (25);
   counter             NUMBER;
   fileid              VARCHAR2 (25);
   v_bin               rec_mduonus_reco_temp.rmr_pan%TYPE;
   vmesgtype           rec_mduonus_reco_temp.rmr_mesg_type%TYPE;
   vbinfor             VARCHAR2 (4);

   CURSOR c
   IS
      SELECT ROWID, a.*
        FROM rec_mduonus_reco_temp a;
BEGIN
   errmsg := 'OK';

   DELETE FROM rec_mduonus_reco_temp
         WHERE TO_NUMBER (rmr_comp_trans_amt) = 0 AND rmr_mesg_type <> 'EREC';

   UPDATE rec_mduonus_reco_temp
      SET rmr_intl_flag = fn_get_domintlflag (rmr_de_43);


   IF errmsg = 'OK'
   THEN
      BEGIN
         SELECT rmr_pds_0105
           INTO fileid
           FROM rec_mduonus_reco_temp
          WHERE rmr_mesg_type = '1644' AND rmr_comp_code = '697';

         DBMS_OUTPUT.put_line ('nachi LASTMDU DATE' || fileid);

         UPDATE rec_mduonus_reco_temp
            SET rmr_pds_0105 = fileid
          WHERE rmr_comp_code NOT IN ('697', '695');

         UPDATE cms_inst_param
            SET cip_param_value = SUBSTR (fileid, 4, 6)
          WHERE cip_param_key = 'LASTMDU';
      EXCEPTION
         WHEN OTHERS
         THEN
            errmsg :=
                  'Error while updating the value for key LASTMDU or selecting LASTMDU'
               || SQLERRM;
      END;
   END IF;

--En Nachiketa , 05  Nov 07, For CR-197

   -- Sn Nachiketa, 21st FEB 2013
/*
