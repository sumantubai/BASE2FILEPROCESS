	"RCR_EXCEPTION_INDICATOR" CHAR(1 BYTE) DEFAULT 'N', 
	"RCR_BRAN_CODE" VARCHAR2(6 BYTE)
   ) ;

  CREATE INDEX "NIDCRS"."INDX_CTF_RECO" ON "NIDCRS"."REC_CTF_RECO" ("RCR_PAN_NUMB", "RCR_PAN_EXTN", "RCR_AUTH_CODE", "RCR_TRAN_CODE", "RCR_USAGE_CODE");
   
CREATE TABLE "NIDCRS"."REC_FILE_DEF" 
   (	"RCD_TRAN_CODE" NUMBER(5,0), 
	"RCD_TCR_NUMB" NUMBER(5,0), 
	"RCD_VISA_FLDNAME" VARCHAR2(255 BYTE), 
	"RCD_POSN" NUMBER(5,0), 
	"RCD_FLD_LEN" NUMBER(5,0), 
	"RCD_FLD_NAME" VARCHAR2(30 BYTE), 
	"RCD_SKIP_FLD" VARCHAR2(1 BYTE), 
	"RCD_FILE_TYPE" VARCHAR2(10 BYTE)
   ) ;   
   
   
create or replace PROCEDURE        Sp_Ins_Ctfreco (ERRMSG        OUT             VARCHAR2)
AS
C_RBR_RECON NUMBER(1):= 6;
BEGIN
        errmsg:= 'OK';
        UPDATE REC_CTF_RECO_TEMP SET rcr_purch_date=SUBSTR(rcr_ctf_file ,5,2)||SUBSTR(rcr_ctf_file ,3,2) ,
    rcr_purch_year = SUBSTR(rcr_ctf_file,7,2)||SUBSTR(rcr_ctf_file ,5,2)||SUBSTR(rcr_ctf_file ,3,2) WHERE rcr_purch_date='0000';
--Imran 28 May 03
---     INSERT INTO REC_CTF_RECO (SELECT * FROM REC_CTF_RECO_TEMP);
--by cHRISTOPHER ON 4TH june
-- AJIT ON 20 JUN 03

--Imran 7 Nov 2003, chk base on ARN+TRANCODE+AMT
/*    UPDATE REC_CTF_RECO_TEMP SET RCR_DUP_FLAG ='Y' WHERE RCR_ACQ_REF_NUMB IN
    (SELECT RCR_ACQ_REF_NUMB FROM REC_CTF_RECO_TEMP WHERE RCR_TRAN_CODE IN( '05','07')
    GROUP BY RCR_ACQ_REF_NUMB  HAVING COUNT(*) > 1 ) AND RCR_TRAN_CODE IN( '05','07');

  UPDATE REC_CTF_RECO_TEMP set rcr_dup_flag='Y' where  RCR_ACQ_REF_NUMB||RCR_TRAN_CODE
    IN (select  RCR_ACQ_REF_NUMB||RCR_TRAN_CODE from rec_ctf_reco where RCR_TRAN_CODE IN( '05','07')) AND RCR_TRAN_CODE IN( '05','07');
*/

    UPDATE REC_CTF_RECO_TEMP SET RCR_DUP_FLAG ='Y' WHERE RCR_ACQ_REF_NUMB||RCR_TRAN_CODE||RCR_DEST_AMT IN
    (SELECT RCR_ACQ_REF_NUMB||RCR_TRAN_CODE||RCR_DEST_AMT FROM REC_CTF_RECO_TEMP WHERE RCR_TRAN_CODE IN( '05','07')
    GROUP BY RCR_ACQ_REF_NUMB||RCR_TRAN_CODE||RCR_DEST_AMT  HAVING COUNT(*) > 1 ) AND RCR_TRAN_CODE IN( '05','07');

    UPDATE REC_CTF_RECO_TEMP SET rcr_dup_flag='Y' WHERE  RCR_ACQ_REF_NUMB||RCR_TRAN_CODE||RCR_DEST_AMT
    IN (SELECT  RCR_ACQ_REF_NUMB||RCR_TRAN_CODE||RCR_DEST_AMT FROM REC_CTF_RECO WHERE RCR_TRAN_CODE IN( '05','07')) AND RCR_TRAN_CODE IN( '05','07');
--Imran 7 Nov 2003, chk base on ARN+TRANCODE+AMT



 UPDATE REC_CTF_RECO_TEMP
       SET RCR_FROM_ACCT = (SELECT CAP_ACCT_NO FROM  CMS_APPL_PAN_ADB WHERE CAP_PAN_CODE=RCR_PAN_NUMB),
         RCR_BRAN_CODE = (SELECT CAP_APPL_BRAN FROM  CMS_APPL_PAN_ADB WHERE CAP_PAN_CODE=RCR_PAN_NUMB);

    UPDATE REC_CTF_RECO_TEMP SET RCR_EXCEPTION_INDICATOR='Y'  WHERE RCR_FROM_ACCT IS NULL;

  INSERT INTO REC_CTF_RECO (SELECT * FROM REC_CTF_RECO_TEMP WHERE RCR_TRAN_CODE IN ('05','06','07','25','26','27'));
  
  INSERT INTO REC_CTF_RECO_VISAOUT (SELECT * FROM REC_CTF_RECO_TEMP WHERE RCR_TRAN_CODE IN ('05','06','07','25','26','27'));

        UPDATE  REC_CTF_RECO_TEMP SET RCR_ADJ_FLAG= 'Y'  WHERE
        RCR_ACQ_REF_NUMB IN
                (SELECT A.RCR_ACQ_REF_NUMB   FROM REC_CTF_RECO_TEMP A , REC_CTF_RECO_TEMP B WHERE
                ((A.RCR_TRAN_CODE = '05' AND B.RCR_TRAN_CODE = '25') OR (A.RCR_TRAN_CODE = '07' AND B.RCR_TRAN_CODE = '27'))
                AND A.RCR_SOURCE_AMT !=B.RCR_SOURCE_AMT AND A.RCR_ACQ_REF_NUMB = B.RCR_ACQ_REF_NUMB);

--Imran 23 Jun 2003
    DELETE FROM REC_BASE2_RECON_TEMP;
-- ADDED BY AJIT AS ON DATE 29 MAY 03

       INSERT INTO REC_BASE2_RECON_TEMP(
                RBR_RECON,RBR_PROCESS_DATE,RBR_CTF_FILE,RBR_TRAN_CODE,
                RBR_TCR_NUMB,RBR_ACQ_BUS_ID,RBR_ACQ_REF_NUMB,RBR_AUTH_CHAR_IND, RBR_AUTH_CODE,
                RBR_BATCH_NUMB,RBR_CARD_ID_METHOD,RBR_CENTER_BATCH_ID,RBR_CENTRAL_PR_DATE,
                RBR_COLL_ONLY_FLAG,RBR_CRB_EXCP,RBR_DEST_AMT,RBR_DEST_CURR,RBR_DEST_BIN,
                RBR_FLOOR_LIMIT, RBR_INC_FILE_ID,RBR_INTL_FEE_IND,RBR_MER_CAT,RBR_MER_CITY,
                RBR_MER_CNTRY,RBR_MER_NAME, RBR_MER_STATE_CODE,RBR_MER_ZIP_CODE,RBR_MESS_TEXT,
                RBR_NO_MON_TRANS,RBR_NO_TRANS,RBR_PAN_NUMB, RBR_PAN_EXTN,RBR_PCAS,
                RBR_POS_ENT_MODE,RBR_POS_TERM_CAP,RBR_PURCH_DATE,RBR_REASON_CODE,
                RBR_REIMB_ATTRIB,RBR_REQ_PAYMENT,RBR_SETTL_DATE,RBR_SETTL_FLAG,RBR_SOURCE_AMT,
                RBR_SOURCE_BIN, RBR_SOURCE_CURR,RBR_TRAN_ID,RBR_USAGE_CODE,
                RBR_CHGBK_REF_NUMB,RBR_DOC_IND,RBR_CTF_TERM_ID,RBR_MAIL_TEL_ECOM_IND,RBR_LOYL_FLAG,RBR_IBR_GEN, RBR_ID_COL/*added on 24-08-02*/,
                RBR_NATIONAL_REIMB_FEE,RBR_ATM_ACCT_SELECTION
                )
            SELECT
                C_RBR_RECON,SYSDATE,C.RCR_CTF_FILE,C.RCR_TRAN_CODE,C.RCR_TCR_NUMB,C.RCR_ACQ_BUS_ID,C.RCR_ACQ_REF_NUMB,C.RCR_AUTH_CHAR_IND,
                C.RCR_AUTH_CODE,C.RCR_BATCH_NUMB,C.RCR_CARD_ID_METHOD,C.RCR_CENTER_BATCH_ID,C.RCR_CENTRAL_PR_DATE,
                C.RCR_COLL_ONLY_FLAG,C.RCR_CRB_EXCP,C.RCR_DEST_AMT,C.RCR_DEST_CURR,C.RCR_DEST_BIN,C.RCR_FLOOR_LIMIT,
                C.RCR_INC_FILE_ID,C.RCR_INTL_FEE_IND,C.RCR_MER_CAT,C.RCR_MER_CITY,C.RCR_MER_CNTRY,C.RCR_MER_NAME,
                C.RCR_MER_STATE_CODE,C.RCR_MER_ZIP_CODE,C.RCR_MESS_TEXT,C.RCR_NO_MON_TRANS,C.RCR_NO_TRANS,
                C.RCR_PAN_NUMB,C.RCR_PAN_EXTN,C.RCR_PCAS,C.RCR_POS_ENT_MODE,C.RCR_POS_TERM_CAP,C.RCR_PURCH_DATE,
                C.RCR_REASON_CODE,C.RCR_REIMB_ATTRIB,C.RCR_REQ_PAYMENT,C.RCR_SETTL_DATE,C.RCR_SETTL_FLAG,
                C.RCR_SOURCE_AMT,C.RCR_SOURCE_BIN,C.RCR_SOURCE_CURR,C.RCR_TRAN_ID,C.RCR_USAGE_CODE,C.RCR_CHGBK_REF_NUMB,
                C.RCR_DOC_IND,C.RCR_TERM_ID,C.RCR_MAIL_TEL_ECOM_IND,'N',0,C.RCR_ID_COL,
                C.RCR_NATIONAL_REIMB_FEE,C.RCR_ATM_ACCT_SELECTION
--Imran 10 Mar 2004
--            FROM    REC_CTF_RECO C
            FROM    REC_CTF_RECO_TEMP C
--Imran 10 Mar 2004
            WHERE    rcr_dup_flag = 'Y';
