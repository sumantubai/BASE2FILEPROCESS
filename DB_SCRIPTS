                         rmr_comp_trans_amt, rmr_atm_access_fee,
                         rmr_inter_fee, rmr_switch_fee, rmr_prod_code,
                         rmr_seg3_curr_code, rmr_seg3_conv_rate,
                         rmr_seg3_cond_code, rmr_ref_numb, rmr_issu_inst,
                         rmr_from_acct, rmr_to_acct, rmr_mcc_code,
                         rmr_acq_inst_name, rmr_acq_city,
                         rmr_switch_serial_numb, rmr_mer_id,
                         rmr_banknet_ref_numb, rmr_acq_ref_numb,
                         rmr_ins_user, rmr_ins_date, rmr_lupd_user,
                         rmr_lupd_date, rmr_settle_date, rmr_iso_resp_code,
                         rmr_match_flag, rmr_intl_flag, rmr_processing_flag,
                         rmr_man_rec, rmr_seq_num, rmr_id_col, rmr_uniq_id,
                         rmr_msg_rsncode, rmr_org_amt1, rmr_org_amt2,
                         rmr_fwdinst_id, rmr_retref_num,
                         rmr_card_accpt_cntry, rmr_msg_num, rmr_iss_refdata,
                         rmr_org_instid, rmr_de_48, rmr_de_43,
                         rmr_crd_accpt_id, rmr_lifecycle_id, rmr_exp_date,
                         rmr_rec_amt, rmr_bill_conv_rate, rmr_rec_curr,
                         rmr_bill_curr, rmr_de_72, rmr_pos_term_capability,
                         rmr_de22_se7, rmr_de22_se8, rmr_de22_se4,
                         rmr_pds_0165, rmr_pds_0176, rmr_pds_0025_se2,
                         rmr_pds_0262, rmr_de_62, rmr_de_123, rmr_de_124,
                         rmr_de_125, rmr_pds_0501, rmr_pds_0025_se1,
                         rmr_ttum_filename, rmr_de_22,
                         rmr_bran_code
               -- Added By Nachiketa , 11 Feb 08 For sending DE22 in IPM File
                                      ,
                         rmr_icc_data, rmr_orig_icc_data)
               (SELECT rmr_inst_code, rmr_reco_flag, rmr_file_name,
                       rmr_mesg_type, rmr_processor_type, rmr_processor_id,
                       rmr_tran_date, rmr_tran_time, rmr_pan, rmr_proc_code,
                       rmr_amt_settle, rmr_trace_numb, rmr_acq_inst_id,
                       rmr_term_id, rmr_resp_cde, rmr_comp_code,
                       rmr_curr_code, rmr_acq_conv_rate, rmr_comp_trans_amt,
                       rmr_atm_access_fee, rmr_inter_fee, rmr_switch_fee,
                       rmr_prod_code, rmr_seg3_curr_code, rmr_seg3_conv_rate,
                       rmr_seg3_cond_code, rmr_ref_numb, rmr_issu_inst,
