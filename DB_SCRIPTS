                    ,RMR_CRD_ACCPT_ID,RMR_LIFECYCLE_ID,RMR_EXP_DATE,RMR_REC_AMT,RMR_BILL_CONV_RATE,RMR_REC_CURR,RMR_BILL_CURR,RMR_DE_72
                    ,RMR_POS_TERM_CAPABILITY,RMR_DE22_SE7,RMR_DE22_SE8,RMR_DE22_SE4,RMR_PDS_0165,RMR_PDS_0176,RMR_PDS_0025_SE2,RMR_PDS_0262
                    ,RMR_DE_62,RMR_DE_123,RMR_DE_124,RMR_DE_125,RMR_PDS_0501,RMR_PDS_0105,RMR_PDS_0025_SE1,RMR_TTUM_FILENAME,RMR_SOURCE_INSTID
