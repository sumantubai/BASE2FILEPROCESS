  CREATE TABLE "NIDCRS"."REC_CTF_RECO" 
   (	"RCR_MAP_CODE" VARCHAR2(6 BYTE), 
	"RCR_RECON" NUMBER(1,0) NOT NULL ENABLE, 
	"RCR_PROC_DATE" DATE NOT NULL ENABLE, 
	"RCR_CTF_FILE" VARCHAR2(25 BYTE) NOT NULL ENABLE, 
	"RCR_TRAN_CODE" VARCHAR2(2 BYTE), 
	"RCR_TCR_NUMB" VARCHAR2(1 BYTE), 
	"RCR_ACQ_BUS_ID" VARCHAR2(8 BYTE), 
	"RCR_ACQ_REF_NUMB" VARCHAR2(23 BYTE), 
	"RCR_AUTH_CHAR_IND" VARCHAR2(1 BYTE), 
	"RCR_AUTH_CODE" VARCHAR2(6 BYTE), 
	"RCR_BATCH_NUMB" VARCHAR2(6 BYTE), 
	"RCR_CARD_ID_METHOD" VARCHAR2(1 BYTE), 
	"RCR_CENTER_BATCH_ID" VARCHAR2(8 BYTE), 
	"RCR_CENTRAL_PR_DATE" VARCHAR2(4 BYTE), 
	"RCR_COLL_ONLY_FLAG" VARCHAR2(1 BYTE), 
	"RCR_CRB_EXCP" VARCHAR2(1 BYTE), 
	"RCR_DEST_AMT" VARCHAR2(15 BYTE), 
	"RCR_DEST_CURR" VARCHAR2(3 BYTE), 
	"RCR_DEST_BIN" VARCHAR2(6 BYTE), 
	"RCR_FLOOR_LIMIT" VARCHAR2(1 BYTE), 
	"RCR_INC_FILE_ID" VARCHAR2(3 BYTE), 
	"RCR_INTL_FEE_IND" VARCHAR2(1 BYTE), 
	"RCR_MER_CAT" VARCHAR2(4 BYTE), 
	"RCR_MER_CITY" VARCHAR2(13 BYTE), 
	"RCR_MER_CNTRY" VARCHAR2(3 BYTE), 
	"RCR_MER_NAME" VARCHAR2(25 BYTE), 
	"RCR_MER_STATE_CODE" VARCHAR2(3 BYTE), 
	"RCR_MER_ZIP_CODE" VARCHAR2(5 BYTE), 
	"RCR_MESS_TEXT" VARCHAR2(107 BYTE), 
	"RCR_NO_MON_TRANS" VARCHAR2(12 BYTE), 
	"RCR_NO_TRANS" VARCHAR2(9 BYTE), 
	"RCR_PAN_NUMB" VARCHAR2(19 BYTE), 
	"RCR_PAN_EXTN" VARCHAR2(3 BYTE), 
	"RCR_PCAS" VARCHAR2(1 BYTE), 
	"RCR_POS_ENT_MODE" VARCHAR2(2 BYTE), 
	"RCR_POS_TERM_CAP" VARCHAR2(1 BYTE), 
	"RCR_PROCESS_DATE" VARCHAR2(5 BYTE), 
	"RCR_PURCH_DATE" VARCHAR2(4 BYTE), 
	"RCR_REASON_CODE" VARCHAR2(4 BYTE), 
	"RCR_REIMB_ATTRIB" VARCHAR2(1 BYTE), 
	"RCR_REQ_PAYMENT" VARCHAR2(1 BYTE), 
	"RCR_SETTL_DATE" VARCHAR2(5 BYTE), 
	"RCR_SETTL_FLAG" VARCHAR2(1 BYTE), 
	"RCR_SOURCE_AMT" VARCHAR2(15 BYTE), 
	"RCR_SOURCE_BIN" VARCHAR2(6 BYTE), 
	"RCR_SOURCE_CURR" VARCHAR2(3 BYTE), 
	"RCR_TRAN_ID" VARCHAR2(15 BYTE), 
	"RCR_USAGE_CODE" VARCHAR2(1 BYTE), 
	"RCR_CHGBK_REF_NUMB" VARCHAR2(6 BYTE), 
	"RCR_DOC_IND" VARCHAR2(1 BYTE), 
	"RCR_TERM_ID" VARCHAR2(8 BYTE), 
	"RCR_PURCH_YEAR" VARCHAR2(6 BYTE), 
	"RCR_ISSUER_WORKSTN_BIN" VARCHAR2(6 BYTE), 
	"RCR_ACQ_WORKSTN_BIN" VARCHAR2(6 BYTE), 
	"RCR_MEMB_MESG_TEXT" VARCHAR2(50 BYTE), 
	"RCR_SPCL_COND_IND" VARCHAR2(2 BYTE), 
	"RCR_RESERVED_TCR1_1" VARCHAR2(5 BYTE), 
	"RCR_CARD_ACCPT_ID" VARCHAR2(15 BYTE), 
	"RCR_NATIONAL_REIMB_FEE" VARCHAR2(12 BYTE), 
	"RCR_MAIL_TEL_ECOM_IND" CHAR(1 BYTE), 
	"RCR_SPCL_CHGBK_IND" CHAR(1 BYTE), 
	"RCR_INTERFACE_TRACE_NUMB" VARCHAR2(6 BYTE), 
	"RCR_CARDHOLD_ACT_TERM_IND" CHAR(1 BYTE), 
	"RCR_PREPAID_CARD_IND" CHAR(1 BYTE), 
	"RCR_SERVICE_DEVLP_FIELD" CHAR(1 BYTE), 
	"RCR_AVS_RESP_CODE" CHAR(1 BYTE), 
	"RCR_AUTH_SOURCE_CODE" CHAR(1 BYTE), 
	"RCR_PURCHASE_ID_FORMAT" CHAR(1 BYTE), 
	"RCR_ATM_ACCT_SELECTION" CHAR(1 BYTE), 
