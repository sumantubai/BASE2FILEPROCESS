 */
-- En Nachiketa, 21st FEB 2013

   -- Sn Nachiketa, 18th Oct 2012
   IF errmsg = 'OK'
   THEN
      counter := 0;

      FOR x IN c
      LOOP
         counter := counter + 1;
         DBMS_OUTPUT.put_line ('counter : ' || counter);
         DBMS_OUTPUT.put_line ('ROWID : ' || x.ROWID);
/*
         BEGIN
            SELECT cbm_bin_for
              INTO vbinfor
              FROM chg_bin_mast
             WHERE cbm_bin_code = SUBSTR (x.rmr_pan, 1, 6) AND ROWNUM = 1;
         EXCEPTION
            WHEN OTHERS
            THEN
               vbinfor := '';
         END;
*/
         DBMS_OUTPUT.put_line ('1');

         BEGIN
            SELECT rmr_mesg_type
              INTO vmesgtype
              FROM rec_mduonus_reco_temp
             WHERE ROWID = x.ROWID;
         EXCEPTION
            WHEN OTHERS
            THEN
               v_bin := '12345';
         END;

         BEGIN
            SELECT SUBSTR (x.rmr_pan, 0, 6)
              INTO v_bin
              FROM rec_mduonus_reco_temp
             WHERE ROWID = x.ROWID AND TRIM (rmr_pan) IS NOT NULL;
         EXCEPTION
            WHEN OTHERS
            THEN
               v_bin := '12345';
         END;

         UPDATE rec_mduonus_reco_temp
            SET rmr_exception_indicator = 'Y'
          WHERE rmr_from_acct IS NULL AND ROWID = x.ROWID;

         IF NVL (v_bin, '000000') IN ('559459', '532749')
         THEN
            INSERT INTO rec_mduonus_reco
                        (rmr_inst_code, rmr_reco_flag, rmr_file_name,
                         rmr_mesg_type, rmr_processor_type, rmr_processor_id,
                         rmr_tran_date, rmr_tran_time, rmr_pan,
                         rmr_proc_code, rmr_amt_settle, rmr_trace_numb,
                         rmr_acq_inst_id, rmr_term_id, rmr_resp_cde,
                         rmr_comp_code, rmr_curr_code, rmr_acq_conv_rate,
                         rmr_comp_trans_amt, rmr_atm_access_fee,
                         rmr_inter_fee, rmr_switch_fee, rmr_prod_code,
                         rmr_seg3_curr_code, rmr_seg3_conv_rate,
                         rmr_seg3_cond_code, rmr_ref_numb, rmr_issu_inst,
                         rmr_from_acct, rmr_to_acct, rmr_mcc_code,
                         rmr_acq_inst_name, rmr_acq_city,
                         rmr_switch_serial_numb, rmr_mer_id,
                         rmr_banknet_ref_numb, rmr_acq_ref_numb,
                         rmr_ins_user, rmr_ins_date, rmr_lupd_user,
                         rmr_lupd_date, rmr_settle_date, rmr_iso_resp_code,
                         rmr_match_flag, rmr_intl_flag, rmr_processing_flag,
                         rmr_man_rec, rmr_seq_num, rmr_id_col, rmr_uniq_id,
                         rmr_msg_rsncode, rmr_org_amt1, rmr_org_amt2,
                         rmr_fwdinst_id, rmr_retref_num,
                         rmr_card_accpt_cntry, rmr_msg_num, rmr_iss_refdata,
                         rmr_org_instid, rmr_de_48, rmr_de_43,
                         rmr_crd_accpt_id, rmr_lifecycle_id, rmr_exp_date,
                         rmr_rec_amt, rmr_bill_conv_rate, rmr_rec_curr,
                         rmr_bill_curr, rmr_de_72, rmr_pos_term_capability,
                         rmr_de22_se7, rmr_de22_se8, rmr_de22_se4,
                         rmr_pds_0165, rmr_pds_0176, rmr_pds_0025_se2,
                         rmr_pds_0262, rmr_de_62, rmr_de_123, rmr_de_124,
                         rmr_de_125, rmr_pds_0501, rmr_pds_0025_se1,
                         rmr_ttum_filename, rmr_de_22, rmr_bran_code,
                         rmr_pds_0105,
                         rmr_source_instid
                                          ,
                         rmr_icc_data, rmr_orig_icc_data)
               (SELECT rmr_inst_code, rmr_reco_flag, rmr_file_name,
                       rmr_mesg_type, rmr_processor_type, rmr_processor_id,
                       rmr_tran_date, rmr_tran_time, rmr_pan, rmr_proc_code,
                       rmr_amt_settle, rmr_trace_numb, rmr_acq_inst_id,
                       rmr_term_id, rmr_resp_cde, rmr_comp_code,
                       rmr_curr_code, rmr_acq_conv_rate, rmr_comp_trans_amt,
                       rmr_atm_access_fee, rmr_inter_fee, rmr_switch_fee,
                       rmr_prod_code, rmr_seg3_curr_code, rmr_seg3_conv_rate,
                       rmr_seg3_cond_code, rmr_ref_numb, rmr_issu_inst,
                       rmr_from_acct, rmr_to_acct, rmr_mcc_code,
                       rmr_acq_inst_name, rmr_acq_city,
                       rmr_switch_serial_numb, rmr_mer_id,
                       rmr_banknet_ref_numb, rmr_acq_ref_numb, rmr_ins_user,
                       rmr_ins_date, rmr_lupd_user, rmr_lupd_date,
                       rmr_settle_date, rmr_iso_resp_code, rmr_match_flag,
                       rmr_intl_flag, rmr_processing_flag, rmr_man_rec,
                       rmr_seq_num, rmr_id_col, rmr_uniq_id, rmr_msg_rsncode,
                       rmr_org_amt1, rmr_org_amt2, rmr_fwdinst_id,
                       rmr_retref_num, rmr_card_accpt_cntry, rmr_msg_num,
                       rmr_iss_refdata, rmr_org_instid, rmr_de_48, rmr_de_43,
                       rmr_crd_accpt_id, rmr_lifecycle_id, rmr_exp_date,
                       rmr_rec_amt, rmr_bill_conv_rate, rmr_rec_curr,
                       rmr_bill_curr, rmr_de_72, rmr_pos_term_capability,
                       rmr_de22_se7, rmr_de22_se8, rmr_de22_se4,
                       rmr_pds_0165, rmr_pds_0176, rmr_pds_0025_se2,
                       rmr_pds_0262, rmr_de_62, rmr_de_123, rmr_de_124,
                       rmr_de_125, rmr_pds_0501, rmr_pds_0025_se1,
                       rmr_ttum_filename, rmr_de_22, rmr_bran_code,
                       rmr_pds_0105,
                       rmr_source_instid
                                        ,
                       rmr_icc_data, rmr_orig_icc_data
                  FROM rec_mduonus_reco_temp
                 WHERE SUBSTR (rmr_resp_cde, 1, 2) IN ('00', '01')
                   AND SUBSTR (rmr_proc_code, 1, 2) NOT IN ('95')
                   AND (   rmr_comp_code NOT IN ('205', '282')
                        OR rmr_comp_code IS NULL
                       )
                   AND rmr_mesg_type NOT IN ('1442', '1644', 'EREC')
                   AND ROWID = x.ROWID);

-- Changes for Nonoo Prepaid
            DBMS_OUTPUT.put_line (   ' nvl(v_bin,000000) : '
                                  || NVL (v_bin, '000000')
                                  || 'Message Type:  '
                                  || vmesgtype
                                 );
         ELSIF NVL (v_bin, '000000') IN ('529070') OR vmesgtype = '1740'
         THEN
            DBMS_OUTPUT.put_line (' FOR NONO :' || vmesgtype);

            INSERT INTO rec_mduonus_reco
                        (rmr_inst_code, rmr_reco_flag, rmr_file_name,
                         rmr_mesg_type, rmr_processor_type, rmr_processor_id,
                         rmr_tran_date, rmr_tran_time, rmr_pan,
                         rmr_proc_code, rmr_amt_settle, rmr_trace_numb,
                         rmr_acq_inst_id, rmr_term_id, rmr_resp_cde,
                         rmr_comp_code, rmr_curr_code, rmr_acq_conv_rate,
                         rmr_comp_trans_amt, rmr_atm_access_fee,
                         rmr_inter_fee, rmr_switch_fee, rmr_prod_code,
                         rmr_seg3_curr_code, rmr_seg3_conv_rate,
                         rmr_seg3_cond_code, rmr_ref_numb, rmr_issu_inst,
                         rmr_from_acct, rmr_to_acct, rmr_mcc_code,
                         rmr_acq_inst_name, rmr_acq_city,
                         rmr_switch_serial_numb, rmr_mer_id,
                         rmr_banknet_ref_numb, rmr_acq_ref_numb,
                         rmr_ins_user, rmr_ins_date, rmr_lupd_user,
                         rmr_lupd_date, rmr_settle_date, rmr_iso_resp_code,
                         rmr_match_flag, rmr_intl_flag, rmr_processing_flag,
                         rmr_man_rec, rmr_seq_num, rmr_id_col, rmr_uniq_id,
                         rmr_msg_rsncode, rmr_org_amt1, rmr_org_amt2,
                         rmr_fwdinst_id, rmr_retref_num,
                         rmr_card_accpt_cntry, rmr_msg_num, rmr_iss_refdata,
                         rmr_org_instid, rmr_de_48, rmr_de_43,
                         rmr_crd_accpt_id, rmr_lifecycle_id, rmr_exp_date,
                         rmr_rec_amt, rmr_bill_conv_rate, rmr_rec_curr,
                         rmr_bill_curr, rmr_de_72, rmr_pos_term_capability,
                         rmr_de22_se7, rmr_de22_se8, rmr_de22_se4,
                         rmr_pds_0165, rmr_pds_0176, rmr_pds_0025_se2,
                         rmr_pds_0262, rmr_de_62, rmr_de_123, rmr_de_124,
                         rmr_de_125, rmr_pds_0501, rmr_pds_0025_se1,
                         rmr_ttum_filename, rmr_de_22, rmr_bran_code,
                         rmr_icc_data, rmr_orig_icc_data, rmr_pds_0146,
                         rmr_pds_0159, rmr_pds_0177    --SN Suresh 06Sept2013
                                                   ,
                         rmr_pds_0105, rmr_dest_instid,
                         rmr_pds_0137             --Added By Suresh 02Dec2013
                                     )
               (SELECT rmr_inst_code, rmr_reco_flag, rmr_file_name,
                       rmr_mesg_type, rmr_processor_type, rmr_processor_id,
                       rmr_tran_date, rmr_tran_time, rmr_pan, rmr_proc_code,
                       rmr_amt_settle, rmr_trace_numb, rmr_acq_inst_id,
                       rmr_term_id, rmr_resp_cde, rmr_comp_code,
                       rmr_curr_code, rmr_acq_conv_rate, rmr_comp_trans_amt,
                       rmr_atm_access_fee, rmr_inter_fee, rmr_switch_fee,
                       rmr_prod_code, rmr_seg3_curr_code, rmr_seg3_conv_rate,
                       rmr_seg3_cond_code, rmr_ref_numb, rmr_issu_inst,
                       rmr_from_acct, rmr_to_acct, rmr_mcc_code,
                       rmr_acq_inst_name, rmr_acq_city,
                       rmr_switch_serial_numb, rmr_mer_id,
                       rmr_banknet_ref_numb, rmr_acq_ref_numb, rmr_ins_user,
                       rmr_ins_date, rmr_lupd_user, rmr_lupd_date,
                       rmr_settle_date, rmr_iso_resp_code, rmr_match_flag,
                       rmr_intl_flag, rmr_processing_flag, rmr_man_rec,
                       rmr_seq_num, rmr_id_col, rmr_uniq_id, rmr_msg_rsncode,
                       rmr_org_amt1, rmr_org_amt2, rmr_fwdinst_id,
                       rmr_retref_num, rmr_card_accpt_cntry, rmr_msg_num,
                       rmr_iss_refdata, rmr_org_instid, rmr_de_48, rmr_de_43,
                       rmr_crd_accpt_id, rmr_lifecycle_id, rmr_exp_date,
                       rmr_rec_amt, rmr_bill_conv_rate, rmr_rec_curr,
                       rmr_bill_curr, rmr_de_72, rmr_pos_term_capability,
                       rmr_de22_se7, rmr_de22_se8, rmr_de22_se4,
                       rmr_pds_0165, rmr_pds_0176, rmr_pds_0025_se2,
                       rmr_pds_0262, rmr_de_62, rmr_de_123, rmr_de_124,
                       rmr_de_125, rmr_pds_0501, rmr_pds_0025_se1,
                       rmr_ttum_filename, rmr_de_22, rmr_bran_code,
                       rmr_icc_data, rmr_orig_icc_data, rmr_pds_0146,
                       rmr_pds_0159, rmr_pds_0177      --SN Suresh 06Sept2013
                                                 ,
                       rmr_pds_0105, rmr_dest_instid,
                       rmr_pds_0137               --Added By Suresh 02Dec2013
                  FROM rec_mduonus_reco_temp
                 WHERE SUBSTR (rmr_resp_cde, 1, 2) IN ('00', '01')
                   AND SUBSTR (rmr_proc_code, 1, 2) NOT IN ('95')
                   AND (   rmr_comp_code NOT IN ('205', '282')
                        OR rmr_comp_code IS NULL
                       )
                   AND rmr_mesg_type NOT IN ('1442', '1640')
                   AND ROWID = x.ROWID);
         --Start For BMI
         ELSIF v_bin IN ('528508', '516054', '512686')
         THEN
            INSERT INTO rec_mduonus_reco
                        (rmr_inst_code, rmr_reco_flag, rmr_file_name,
                         rmr_mesg_type, rmr_processor_type, rmr_processor_id,
                         rmr_tran_date, rmr_tran_time, rmr_pan,
                         rmr_proc_code, rmr_amt_settle, rmr_trace_numb,
                         rmr_acq_inst_id, rmr_term_id, rmr_resp_cde,
                         rmr_comp_code, rmr_curr_code, rmr_acq_conv_rate,
                         rmr_comp_trans_amt, rmr_atm_access_fee,
                         rmr_inter_fee, rmr_switch_fee, rmr_prod_code,
                         rmr_seg3_curr_code, rmr_seg3_conv_rate,
                         rmr_seg3_cond_code, rmr_ref_numb, rmr_issu_inst,
                         rmr_from_acct, rmr_to_acct, rmr_mcc_code,
                         rmr_acq_inst_name, rmr_acq_city,
                         rmr_switch_serial_numb, rmr_mer_id,
                         rmr_banknet_ref_numb, rmr_acq_ref_numb,
                         rmr_ins_user, rmr_ins_date, rmr_lupd_user,
                         rmr_lupd_date, rmr_settle_date, rmr_iso_resp_code,
                         rmr_match_flag, rmr_intl_flag, rmr_processing_flag,
                         rmr_man_rec, rmr_seq_num, rmr_id_col, rmr_uniq_id,
                         rmr_msg_rsncode, rmr_org_amt1, rmr_org_amt2,
                         rmr_fwdinst_id, rmr_retref_num,
                         rmr_card_accpt_cntry, rmr_msg_num, rmr_iss_refdata,
                         rmr_org_instid, rmr_de_48, rmr_de_43,
                         rmr_crd_accpt_id, rmr_lifecycle_id, rmr_exp_date,
                         rmr_rec_amt, rmr_bill_conv_rate, rmr_rec_curr,
                         rmr_bill_curr, rmr_de_72, rmr_pos_term_capability,
                         rmr_de22_se7, rmr_de22_se8, rmr_de22_se4,
                         rmr_pds_0165, rmr_pds_0176, rmr_pds_0025_se2,
                         rmr_pds_0262, rmr_de_62, rmr_de_123, rmr_de_124,
                         rmr_de_125, rmr_pds_0501, rmr_pds_0025_se1,
                         rmr_ttum_filename, rmr_de_22, rmr_bran_code,
                         rmr_icc_data, rmr_orig_icc_data)
               (SELECT rmr_inst_code, rmr_reco_flag, rmr_file_name,
                       rmr_mesg_type, rmr_processor_type, rmr_processor_id,
                       rmr_tran_date, rmr_tran_time, rmr_pan, rmr_proc_code,
                       rmr_amt_settle, rmr_trace_numb, rmr_acq_inst_id,
                       rmr_term_id, rmr_resp_cde, rmr_comp_code,
                       rmr_curr_code, rmr_acq_conv_rate, rmr_comp_trans_amt,
                       rmr_atm_access_fee, rmr_inter_fee, rmr_switch_fee,
                       rmr_prod_code, rmr_seg3_curr_code, rmr_seg3_conv_rate,
                       rmr_seg3_cond_code, rmr_ref_numb, rmr_issu_inst,
                       rmr_from_acct, rmr_to_acct, rmr_mcc_code,
                       rmr_acq_inst_name, rmr_acq_city,
                       rmr_switch_serial_numb, rmr_mer_id,
                       rmr_banknet_ref_numb, rmr_acq_ref_numb, rmr_ins_user,
                       rmr_ins_date, rmr_lupd_user, rmr_lupd_date,
                       rmr_settle_date, rmr_iso_resp_code, rmr_match_flag,
                       rmr_intl_flag, rmr_processing_flag, rmr_man_rec,
                       rmr_seq_num, rmr_id_col, rmr_uniq_id, rmr_msg_rsncode,
                       rmr_org_amt1, rmr_org_amt2, rmr_fwdinst_id,
                       rmr_retref_num, rmr_card_accpt_cntry, rmr_msg_num,
                       rmr_iss_refdata, rmr_org_instid, rmr_de_48, rmr_de_43,
                       rmr_crd_accpt_id, rmr_lifecycle_id, rmr_exp_date,
                       rmr_rec_amt, rmr_bill_conv_rate, rmr_rec_curr,
                       rmr_bill_curr, rmr_de_72, rmr_pos_term_capability,
                       rmr_de22_se7, rmr_de22_se8, rmr_de22_se4,
                       rmr_pds_0165, rmr_pds_0176, rmr_pds_0025_se2,
                       rmr_pds_0262, rmr_de_62, rmr_de_123, rmr_de_124,
                       rmr_de_125, rmr_pds_0501, rmr_pds_0025_se1,
                       rmr_ttum_filename, rmr_de_22, rmr_bran_code,
                       rmr_icc_data, rmr_orig_icc_data
                  FROM rec_mduonus_reco_temp
                 WHERE SUBSTR (rmr_resp_cde, 1, 2) IN ('00', '01')
                   AND SUBSTR (rmr_proc_code, 1, 2) NOT IN ('95')
                   AND (   rmr_comp_code NOT IN ('205', '282')
                        OR rmr_comp_code IS NULL
                       )
                   AND rmr_mesg_type NOT IN ('1442', '1640')
                   AND ROWID = x.ROWID);
         --END
         ELSE
            INSERT INTO rec_mduonus_reco
                        (rmr_inst_code, rmr_reco_flag, rmr_file_name,
                         rmr_mesg_type, rmr_processor_type, rmr_processor_id,
                         rmr_tran_date, rmr_tran_time, rmr_pan,
                         rmr_proc_code, rmr_amt_settle, rmr_trace_numb,
                         rmr_acq_inst_id, rmr_term_id, rmr_resp_cde,
