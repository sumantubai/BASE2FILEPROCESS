                    ,RMR_PDS_0300,RMR_PDS_0301,RMR_PDS_0302,RMR_PDS_0378,RMR_PDS_0380,RMR_PDS_0381,RMR_PDS_0384
                    ,RMR_PDS_0390,RMR_PDS_0391,RMR_PDS_0392,RMR_PDS_0393,RMR_PDS_0394,RMR_PDS_0395,RMR_PDS_0396
                    ,RMR_PDS_0400,RMR_PDS_0401,RMR_PDS_0402
              from REC_MDUONUS_RECO_TEMP;

