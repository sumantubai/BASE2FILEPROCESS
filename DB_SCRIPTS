   IF errmsg = 'OK'
   THEN
      BEGIN
         INSERT INTO rec_mduonus_reco_all
            SELECT *
              FROM rec_mduonus_reco_temp;
      EXCEPTION
         WHEN OTHERS
         THEN
            errmsg :=
                  'ERROR while inserting data into REC_MDUONUS_RECO_ALL table'
               || SQLERRM;
      END;
   END IF;



