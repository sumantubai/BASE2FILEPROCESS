                       rmr_intl_flag, rmr_processing_flag, rmr_man_rec,
                       rmr_seq_num, rmr_id_col, rmr_uniq_id, rmr_msg_rsncode,
                       rmr_org_amt1, rmr_org_amt2, rmr_fwdinst_id,
                       rmr_retref_num, rmr_card_accpt_cntry, rmr_msg_num,
                       rmr_iss_refdata, rmr_org_instid, rmr_de_48, rmr_de_43,
                       rmr_crd_accpt_id, rmr_lifecycle_id, rmr_exp_date,
                       rmr_rec_amt, rmr_bill_conv_rate, rmr_rec_curr,
                       rmr_bill_curr, rmr_de_72, rmr_pos_term_capability,
                       rmr_de22_se7, rmr_de22_se8, rmr_de22_se4,
