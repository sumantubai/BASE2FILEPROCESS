	"RCR_PURCHASE_ID" VARCHAR2(25 BYTE), 
	"RCR_CASHBACK" VARCHAR2(9 BYTE), 
	"RCR_CHIP_COND_CODE" CHAR(1 BYTE), 
	"RCR_RESERVRD_TCR1_2" CHAR(1 BYTE), 
