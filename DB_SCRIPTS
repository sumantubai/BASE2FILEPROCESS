                 WHERE SUBSTR (rmr_resp_cde, 1, 2) IN ('00', '01')
                   AND SUBSTR (rmr_proc_code, 1, 2) NOT IN
                                                     ('95', '19', '29', '20')
                   AND (   rmr_comp_code NOT IN ('205', '282')
                        OR rmr_comp_code IS NULL
                       )
                   AND (rmr_pds_0025_se1 != 'R' OR rmr_pds_0025_se1 IS NULL
                       )                                        -- To be added
                   AND rmr_mesg_type NOT IN ('1442', '1640', '1740', 'EREC')
                   AND ROWID = x.ROWID);
         END IF;
      END LOOP;
   END IF;

