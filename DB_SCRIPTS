              rmr_mesg_type, rmr_processor_type, rmr_processor_id,
              rmr_tran_date, rmr_tran_time, rmr_pan, rmr_proc_code,
              rmr_amt_settle, rmr_trace_numb, rmr_acq_inst_id, rmr_term_id,
              rmr_resp_cde, rmr_comp_code, rmr_curr_code, rmr_acq_conv_rate,
              rmr_comp_trans_amt, rmr_atm_access_fee, rmr_inter_fee,
              rmr_switch_fee, rmr_prod_code, rmr_seg3_curr_code,
              rmr_seg3_conv_rate, rmr_seg3_cond_code, rmr_ref_numb,
              rmr_issu_inst, rmr_from_acct, rmr_to_acct, rmr_mcc_code,
              rmr_acq_inst_name, rmr_acq_city, rmr_switch_serial_numb,
              rmr_mer_id, rmr_banknet_ref_numb, rmr_acq_ref_numb,
              rmr_ins_user, rmr_ins_date, rmr_lupd_user, rmr_lupd_date,
              rmr_settle_date, rmr_iso_resp_code, rmr_match_flag,
              rmr_intl_flag, rmr_processing_flag, rmr_man_rec, rmr_seq_num,
              rmr_id_col, rmr_uniq_id, rmr_msg_rsncode, rmr_org_amt1,
              rmr_org_amt2, rmr_fwdinst_id, rmr_retref_num,
              rmr_card_accpt_cntry, rmr_msg_num, rmr_iss_refdata,
              rmr_org_instid, rmr_de_48, rmr_de_43, rmr_crd_accpt_id,
              rmr_lifecycle_id, rmr_exp_date, rmr_rec_amt, rmr_bill_conv_rate,
              rmr_rec_curr, rmr_bill_curr, rmr_de_72, 'N', NULL, NULL, NULL
                                                                           --added by kirti to add NrREc records 13july07
              ,
              rmr_erec_accfee_ind, rmr_erec_compamt_id, rmr_erec_intfee_id,
              rmr_erec_newrsn_code, rmr_erec_replamt_local,
              rmr_erec_repamt_id, rmr_erec_replamt_settle,
              rmr_erec_repamt_settleid,
              TO_DATE
                 (v_lastmdufiledate, 'YYMMDD')
                  -- Added By Nachiketa , 18 Dec 07,For Accessing Process Date
         FROM rec_mduonus_reco_temp
        WHERE (   (rmr_comp_code IN ('205', '282') AND rmr_mesg_type = '1240'
                  )
               OR (    SUBSTR (rmr_advrsn_code, 4, 4) = '0013'
                   AND rmr_mesg_type = 'EREC'
                  )
              ));
        --by Kirti 19july07 to add representment txns - ADV_REASON,Code = 0013
EXCEPTION
   WHEN OTHERS
   THEN
      errmsg := 'Exep Main ......' || SQLERRM;
END; 