THEN
    BEGIN
         insert into rec_mduonus_reco_all
                    (RMR_INST_CODE ,RMR_RECO_FLAG,RMR_FILE_NAME,RMR_MESG_TYPE,RMR_PROCESSOR_TYPE ,RMR_PROCESSOR_ID
